{
	"name": "Hype",
	"type": "dark",
	"colors": {
		"editor.background": "#111118",
		"editorHoverWidget.background": "#111118",
		"sideBar.background": "#0d0d14",
		"editorGroupHeader.tabsBackground": "#0d0d14",
		"editorGroupHeader.noTabsBackground": "#111118",
		"tab.inactiveBackground": "#0d0d14",
		"tab.border": "#0000",
		"tab.activeBackground": "#111118",
		"tab.activeBorder": "#f38546",
		"activityBar.background": "#0b0b12",
		"titleBar.activeBackground": "#0b0b12",
		"titleBar.inactiveBackground": "#0b0b12",
		"titleBar.inactiveForeground": "#444",
		"activityBarBadge.background": "#fc593d",
		"statusBar.background": "#181825",
		"sideBarTitle.foreground": "#bbbbbb",
		"editor.foreground": "#eff",
		"editor.lineHighlightBackground": "#fff1",
		"editor.lineHighlightBorder": "#0003",
		"editorLineNumber.foreground": "#fff4"
	},
	"tokenColors": [
		{
			"scope": ["comment"],
			"settings": {
				"foreground": "#525263",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["keyword.control", "source.rust keyword.other", "entity.other.attribute-name", "variable.language.super", "keyword.operator.new", "keyword.operator.expression"],
			"settings": {
				"foreground": "#f38546",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["storage.modifier", "storage.type", "entity.other.attribute-name.class.css", "keyword.other.unit"],
			"settings": {
				"foreground": "#f36147",
				"fontStyle": ""
			}
		},
		{
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#ca7bff"
			}
		},
		{
			"scope": ["string", "storage.modifier.lifetime", "entity.name.tag.wildcard", "support.constant.property-value.css"],
			"settings": {
				"foreground": "#c0f348"
			}
		},
		{
			"scope": ["constant.numeric", "support.class.component"],
			"settings": {
				"foreground": "#ff5c9a"
			}
		},
		{
			"name": "params",
			"scope": ["variable.parameter", "entity.other.attribute-name.pseudo-class.css"],
			"settings": {
				"foreground": "#34fa90",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["meta.definition", "constant.character.entity"],
			"settings": {
				"foreground": "#6eb2ff"
			}
		},
		{
			"scope": ["variable.language", "entity.name.type.instance.jsdoc"],
			"settings": {
				"foreground": "#32a3ff",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["constant.language", "entity.name.type"],
			"settings": {
				"foreground": "#ff5765",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["keyword.operator.assignment", "keyword.operator.arithmetic", "punctuation.terminator", "punctuation.accessor", "keyword.operator", "punctuation.separator", "punctuation.definition.bracket.curly", "support.type.property-name.css"],
			"settings": {
				"foreground": "#a0c4ff",
				"fontStyle": ""
			}
		},
		{
			"scope": ["meta.function-call entity.name.function", "entity.name.function.rust", "entity.other.inherited-class", "meta.definition.method"],
			"settings": {
				"foreground": "#ffc955"
			}
		}
	]
}